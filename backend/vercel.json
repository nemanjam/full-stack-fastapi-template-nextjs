{
  "name": "api-full-stack-fastapi-template-nextjs",
  "builds": [
    {
      "src": "backend/app/api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/app/api/index.py"
    }
  ],
    "functions": {
    "backend/app/api/**": {
      "excludeFiles": "{.venv,node_modules,__pycache__,*.pyc,.pytest_cache,tests,docs,htmlcov,.coverage,.env,data}/**"
    }
  }
}
